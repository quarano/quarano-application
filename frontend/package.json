{
  "name": "quarano-frontend",
  "version": "0.0.0",
  "license": "EUPL-1.2-or-later",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "build-mannheim-prod": "ng run quarano-frontend:build:mannheim-prod --baseHref=/mannheim/",
    "build-mannheim-stage": "ng run quarano-frontend:build:mannheim-stage --baseHref=/mannheim-stage/",
    "build-cuxhaven-prod": "ng run quarano-frontend:build:cuxhaven-prod --baseHref=/cuxhaven/",
    "build-cuxhaven-stage": "ng run quarano-frontend:build:cuxhaven-stage --baseHref=/cuxhaven-stage/",
    "build-internal-dev-otc": "ng run quarano-frontend:build:internal-dev-otc --baseHref=/",
    "build-internal-pentest": "ng run quarano-frontend:build:internal-pentest --baseHref=/internal-pentest/",
    "build-internal-test-otc": "ng run quarano-frontend:build:internal-test-otc --baseHref=/internal-test/",
    "build-demo-otc": "ng run quarano-frontend:build:demo-otc --baseHref=/demo/",
    "prepare-deployment-otc": "ncp ./deployment/otc/ ./dist/apps/quarano-frontend/",
    "test": "ng test",
    "ci-test": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng test -- --silent --verbose",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "e2e:watch": "ng e2e --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js",
    "i18n:init": "ngx-translate-extract --input ./apps/quarano-frontend/src ./libs --output ./apps/quarano-frontend/src/assets/i18n/template.json --key-as-default-value --replace --format namespaced-json --key-as-default-value",
    "i18n:extract": "ngx-translate-extract --input ./apps/quarano-frontend/src ./libs --output ./apps/quarano-frontend/src/assets/i18n/template.json ./apps/quarano-frontend/src/assets/i18n/en.json ./apps/quarano-frontend/src/assets/i18n/de.json ./apps/quarano-frontend/src/assets/i18n/tr.json --sort --format namespaced-json --key-as-default-value",
    "workspace-generator": "nx workspace-generator"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(ts|scss|html)'"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.7",
    "@angular/cdk": "11.0.0",
    "@angular/common": "11.0.7",
    "@angular/compiler": "11.0.7",
    "@angular/core": "11.0.7",
    "@angular/forms": "11.0.7",
    "@angular/material": "11.0.0",
    "@angular/material-moment-adapter": "11.0.0",
    "@angular/platform-browser": "11.0.7",
    "@angular/platform-browser-dynamic": "11.0.7",
    "@angular/router": "11.0.7",
    "@ngrx/data": "10.0.1",
    "@ngrx/effects": "10.0.1",
    "@ngrx/entity": "10.0.1",
    "@ngrx/router-store": "10.0.1",
    "@ngrx/store": "10.0.1",
    "@ngrx/store-devtools": "10.0.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@nrwl/angular": "11.1.1",
    "ag-grid-angular": "24.1.0",
    "ag-grid-community": "24.1.0",
    "bootstrap-grid-only-css": "4.1.3",
    "core-js": "2.5.4",
    "country-flag-icons": "1.2.6",
    "dayjs": "1.10.3",
    "file-saver": "2.0.2",
    "jwt-decode": "3.1.1",
    "moment": "2.29.1",
    "nan": "2.14.2",
    "ngx-material-timepicker": "^5.5.3",
    "rxjs": "6.6.3",
    "subsink": "1.0.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1100.6",
    "@angular/cli": "11.0.6",
    "@angular/compiler-cli": "11.0.7",
    "@angular/language-service": "11.0.7",
    "@biesbjerg/ngx-translate-extract": "7.0.2",
    "@cypress/code-coverage": "3.8.1",
    "@nrwl/cli": "11.1.1",
    "@nrwl/cypress": "11.1.1",
    "@nrwl/jest": "11.1.1",
    "@nrwl/tao": "11.1.1",
    "@nrwl/workspace": "11.1.1",
    "@types/file-saver": "2.0.1",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "clipboardy": "2.3.0",
    "codelyzer": "6.0.1",
    "cypress": "6.7.1",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-cypress": "2.10.3",
    "husky": "4.3.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jest": "26.2.2",
    "jest-preset-angular": "8.2.1",
    "jest-zone-patch": "0.0.10",
    "ncp": "2.0.0",
    "ngx-build-plus": "10.1.1",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
  }
}
